<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eprescription.tcc.ufam.com.e_prescription">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.PatientHomeActivity"
            android:label="@string/title_activity_patient_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.PatientRegisterActivity" />
        <activity android:name=".Activities.DoctorRegisterActivity" />
        <activity
            android:name=".Activities.PatientHomeDrawerActivity"
            android:label="@string/title_activity_patient_home_drawer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>